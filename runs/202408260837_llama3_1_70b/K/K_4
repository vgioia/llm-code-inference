```python
import sys
from collections import defaultdict
import math

mod = 998244353
fact = [1]

for i in range(1, int(1e5)+5):
    fact.append((fact[-1] * i) % mod)

def modInv(n, p):
    return pow(n, p-2, p)

def choose(n, k):
    if n < k or k < 0:
        return 0
    else:
        return ((fact[n] * modInv(fact[k], mod)) % mod * modInv(fact[n-k], mod)) % mod

n, k = map(int, sys.stdin.readline().split())

p1 = list(map(int, sys.stdin.readline().split()))
p2 = list(map(int, sys.stdin.readline().split()))

for i in range(n+1):
    res = 0
    for j in range(i+1):
        a = choose(i, j)
        a *= p1[j]
        a %= mod
        a *= modInv(fact[i-j], mod)
        a %= mod
        b = p2[i-j]
        if j%2==k%2:
            res += a * b
        else:
            res -= a * b
        res %= mod
    if i > 0:
        print(res, end=" ")
    else:
        print(res, end="")
```