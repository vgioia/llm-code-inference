```python
import math

def dist(x1, y1, x2, y2):
    return math.sqrt((x1 - x2) ** 2 + (y1 - y2) ** 2)

def check(x1, y1, x2, y2, x3, y3, x4, y4, xj, yj, d):
    return (min(max(dist(x1, y1, xj, yj), dist(x2, y2, xj, yj)),
                max(dist(x1, y1, xj, yj), dist(x3, y3, xj, yj)),
                max(dist(x2, y2, xj, yj), dist(x4, y4, xj, yj))) <= d)

def solve(x1, y1, x2, y2, x3, y3, x4, y4, xj, yj):
    low = 0
    high = max(abs(x1 - x2), abs(y1 - y4))
    for _ in range(100):
        mid = (low + high) / 2
        if check(x1, y1, x2, y2, x3, y3, x4, y4, xj, yj, mid):
            high = mid
        else:
            low = mid
    return high

x1, y1 = map(int, input().split())
x2, y2 = map(int, input().split())
x3, y3 = map(int, input().split())
x4, y4 = map(int, input().split())
n = int(input())
xj = []
yj = []
for _ in range(n):
    x, y = map(int, input().split())
    xj.append(x)
    yj.append(y)

ans = 0
for i in range(n):
    ans = max(ans, solve(x1, y1, x2, y2, x3, y3, x4, y4, xj[i], yj[i]))

print(ans)
```